{
  "extends": [
    "prettier",
    "prettier/flowtype",
    "prettier/react"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": ["flowtype", "react", "prettier"],
  "rules": {
    "react/jsx-filename-extension": [
      2,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/forbid-prop-types": [0],
    "react/require-default-props": [0],
    "global-require": [0],
    "space-in-parens": ["error", "never"],
    "funtion-paren-newline": "never"
  }
}
